      <Type>String</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>inspireId</Name>
      <ElementPath>inspireId|Identifier|localId</ElementPath>
      <Type>String</Type>
    </PropertyDefn>
    <PropertyDefn>
