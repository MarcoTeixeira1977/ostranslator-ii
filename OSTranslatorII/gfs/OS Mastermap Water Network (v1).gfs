      <Type>String</Type>
    </PropertyDefn>
    <PropertyDefn>
      <Name>hydroNodeCategory</Name>
      <ElementPath>hydroNodeCategory</ElementPath>
      <Type>String</Type>
    </PropertyDefn>
  </GMLFeatureClass>
  
